{
  "name": "universal-mod-template-generator",
  "version": "1.0.0",
  "description": "This is a static website that generates a customized Minecraft mod template for multiple mod loaders (Fabric, Forge, NeoForge) and Minecraft versions.",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "deploy": "vercel --prod",
    "generate-og": "node scripts/generate-og-image.js",
    "generate-favicon": "node scripts/generate-favicon.js",
    "generate-assets": "npm run generate-og && npm run generate-favicon"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^1.0.2",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "puppeteer": "^22.15.0",
    "vercel": "^33.5.1"
  }
}
